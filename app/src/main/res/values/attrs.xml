<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShadowLayout">
        <attr name="sl_cornerRadius" format="dimension" />
        <attr name="sl_shadowRadius" format="dimension" />
        <attr name="sl_shadowColor" format="color" />
        <attr name="sl_dx" format="dimension" />
        <attr name="sl_dy" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ExpandTextView">
        <attr name="showLines" format="integer" />
    </declare-styleable>
    <declare-styleable name="PraiseListView">
        <attr name="item_color" format="color" />
        <attr name="item_selector_color" format="color" />
    </declare-styleable>
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>
    <declare-styleable name="MyCalendar">
        <attr name="dateformat" format="string"/>
        <attr name="titleSize" format="dimension"/>
        <attr name="titleColor" format="color"/>
        <attr name="goIcon" format="reference"/>
        <attr name="preIcon" format="reference"/>
        <attr name="dayInMonthColor" format="color"/>
        <attr name="dayOutMonthcolor" format="color"/>
        <attr name="todayColor" format="color"/>
        <attr name="todayEmptycircleColor" format="color"/>
        <attr name="todayFillcircleColor" format="color"/>
        <attr name="calendarbackground" format="color|reference"/>
    </declare-styleable>
    <attr name="SwipeBackLayoutStyle" format="reference" />

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="slidingBlock" format="reference|color" />
        <attr name="allowWidthFull" format="boolean" />
        <attr name="disableViewPager" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RefreshLayout">
        <attr name="footer_view_background" format="color" />
        <attr name="footer_view_text_color" format="color" />
        <attr name="is_need_footer" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CirclePagerIndicator">
        <attr name="circle_indicator_centerHorizontal" format="boolean" />
        <attr name="circle_indicator_follow" format="boolean" />
        <attr name="circle_indicator_orientation" format="reference" />
        <attr name="circle_indicator_color" format="color" />
        <attr name="circle_indicator_stroke_color" format="color" />
        <attr name="circle_indicator_stroke_width" format="dimension" />
        <attr name="circle_indicator_fill_color" format="color" />
        <attr name="circle_indicator_radius" format="dimension" />
        <attr name="circle_indicator_space" format="dimension" />
        <attr name="circle_indicator_indicator_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LinePagerIndicator">
        <attr name="line_indicator_centerHorizontal" format="boolean" />
        <attr name="line_indicator_width" format="dimension" />
        <attr name="line_indicator_height" format="dimension" />
        <attr name="line_indicator_indicator_height" format="dimension" />
        <attr name="line_indicator_space" format="dimension" />
        <attr name="line_indicator_fill_color" format="color" />
        <attr name="line_indicator_indicator_color" format="color" />
        <attr name="line_indicator_follow" format="boolean" />
    </declare-styleable>

    <attr name="verticalSpace" format="dimension" />
    <attr name="horizontalSpace" format="dimension" />

    <declare-styleable name="TweetPicturesLayout">
        <attr name="verticalSpace" />
        <attr name="horizontalSpace" />
        <attr name="column" format="integer" />
        <attr name="maxPictureSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="aTitle" format="string" />
        <attr name="aIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircleBannerIndicator">
        <attr name="circle_banner_indicator_centerHorizontal" format="boolean" />
        <attr name="circle_banner_indicator_orientation" format="reference" />
        <attr name="circle_banner_indicator_color" format="color" />
        <attr name="circle_banner_indicator_stroke_color" format="color" />
        <attr name="circle_banner_indicator_stroke_width" format="dimension" />
        <attr name="circle_banner_indicator_fill_color" format="color" />
        <attr name="circle_banner_indicator_radius" format="dimension" />
        <attr name="circle_banner_indicator_space" format="dimension" />
        <attr name="circle_banner_indicator_indicator_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BannerView">
        <attr name="horizontal_offset" format="dimension" />
        <attr name="vertical_offset" format="dimension" />
        <attr name="scroll_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="IdentityView">
        <attr name="oscColor" format="color" />
        <attr name="oscWipeOffBorder" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BottomLineEditText">
        <attr name="line_color" format="color" />
        <attr name="line_height" format="dimension" />
        <attr name="max_count" format="integer" />
        <attr name="is_show_count" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RatioLayout">
        <attr name="layout_ratio" format="float" />
        <attr name="ratio_flag">
            <enum name="width" value="1" />
            <enum name="height" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="OSCTextView">
        <attr name="font_type">
            <enum name="type_system" value="0" />
            <enum name="type_light" value="1" />
            <enum name="type_medium" value="2" />
            <enum name="type_regular" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="type" />
        <attr name="scale" />
        <attr name="borderRadius" />
        <attr name="top_left" />
        <attr name="top_right" />
        <attr name="bottom_left" />
        <attr name="bottom_right" />
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <attr name="scale">
        <enum name="crop" value="0" />
        <enum name="fit" value="1" />
    </attr>

    <attr name="top_left" format="boolean" />
    <attr name="top_right" format="boolean" />
    <attr name="bottom_left" format="boolean" />
    <attr name="bottom_right" format="boolean" />
    <declare-styleable name="PileLayout">
        <attr name="PileLayout_vertivalSpace" format="dimension" />
        <attr name="PileLayout_pileWidth" format="dimension" />
    </declare-styleable>
</resources>