<?xml version="1.0" encoding="utf-8"?>
<com.lianren.android.widget.invitetime.CalendarDayRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/size_20"
    android:gravity="center"
    android:id="@+id/calendar_day_rl"
    android:layout_marginTop="@dimen/space_8"
    android:layout_marginBottom="@dimen/space_8"
    >

    <com.lianren.android.widget.invitetime.CalendarDayTextView
        android:id="@+id/calendar_day_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/size_20"
        android:minWidth="@dimen/size_20"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:textColor="@color/white"
        android:text="31"
        android:includeFontPadding="false"
        android:textSize="@dimen/font_14"/>
</com.lianren.android.widget.invitetime.CalendarDayRelativeLayout>
